    "library:publish": "yarn publish ./build --non-interactive",
    "library:publish-beta": "yarn publish ./build --tag beta --non-interactive",
    "storybook": "start-storybook -p 6006 -c .storybook watch-css -s src/assets",
    "build-storybook": "build-storybook -s src/assets",
    "library:test": "NODE_ENV=testing jest",
    "library:test:watch": "NODE_ENV=testing jest --watch",
    "new:component": "hygen new component"
